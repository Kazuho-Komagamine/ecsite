function header(){
	/**
	 * ヘッダー
	 */
	var html = "";
	html += '<div id="header">';
	html += '<div id="pr"></div>';
	html += '</div>';
	document.write(html);
}

function footer(){
	/**
	 * フッター
	 */
	var html = "";
	html += '<div id="footer">';
	html += '<div id="pr"></div>';
	html += '</div>';
	document.write(html);
}